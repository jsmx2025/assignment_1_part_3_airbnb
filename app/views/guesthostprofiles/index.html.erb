<div class="page-header">
  <h1>
    Guesthostprofiles
    <a href="/guesthostprofiles/new" class="btn btn-lg btn-success">New Guesthostprofile</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Host</th>
        <th>User guest</th>
        <th>Actions</th>
      </tr>

      <% @guesthostprofiles.each do |guesthostprofile| %>
      <tr>
        <td>
          <% if guesthostprofile.host.present? %>
            <a href="/user_hosts/<%= guesthostprofile.host.id %>">
              <%= guesthostprofile.host.created_at %>
            </a>
          <% end %>
        </td>
        <td>
          <% if guesthostprofile.guest.present? %>
            <a href="/user_guests/<%= guesthostprofile.guest.id %>">
              <%= guesthostprofile.guest.created_at %>
            </a>
          <% end %>
        </td>
        <td>
          <a href="/guesthostprofiles/<%= guesthostprofile.id %>" class="btn btn-primary">Show</a>
          <a href="/guesthostprofiles/<%= guesthostprofile.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_guesthostprofile/<%= guesthostprofile.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @guesthostprofiles, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
