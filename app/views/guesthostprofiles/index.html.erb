<div class="page-header">
  <h1>
    Guesthostprofiles
    <a href="/guesthostprofiles/new" class="btn btn-lg btn-success">New Guesthostprofile</a>
  </h1>
</div><div class="row mb-1">
  <div class="col-md-12">
    <%= search_form_for @q, :class => collapse_search_form, :id => "guesthostprofiles_filters" do |f| %>
      <p class="lead">Narrow results:</p>

        <div class="form-group">
          <%= f.label :created_at, "Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='created_atgteqPicker'>
                  <%= f.text_field :created_at_gteq, :class => "form-control", :placeholder => "Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='created_atlteqPicker'>
                  <%= f.text_field :created_at_lteq, :class => "form-control", :placeholder => "Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#created_atgteqPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#created_atlteqPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#created_atgteqPicker').on("dp.change", function (e) {
                  $('#created_atlteqPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#created_atlteqPicker').on("dp.change", function (e) {
                  $('#created_atgteqPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

        <div class="form-group">
          <%= f.label :created_at, "UserHost Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='hostgteqcreated_atPicker'>
                  <%= f.text_field :host_created_at_gteq, :class => "form-control", :placeholder => "UserHost Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='hostlteqcreated_atPicker'>
                  <%= f.text_field :host_created_at_lteq, :class => "form-control", :placeholder => "UserHost Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#hostgteqcreated_atPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#hostlteqcreated_atPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#hostgteqcreated_atPicker').on("dp.change", function (e) {
                  $('#hostlteqcreated_atPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#hostlteqcreated_atPicker').on("dp.change", function (e) {
                  $('#hostgteqcreated_atPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

<div class="form-group">
          <%= f.label :created_at, "UserGuest Created at" %>
          <div class="row">
            <div class='col-md-6 mb-1'>
              <div class='input-group date' id='guestgteqcreated_atPicker'>
                  <%= f.text_field :guest_created_at_gteq, :class => "form-control", :placeholder => "UserGuest Created at greater than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>

            <div class='col-md-6'>
              <div class='input-group date' id='guestlteqcreated_atPicker'>
                  <%= f.text_field :guest_created_at_lteq, :class => "form-control", :placeholder => "UserGuest Created at less than or equal to" %>
                  <span class="input-group-addon">
                      <span class="glyphicon glyphicon-calendar"></span>
                  </span>
              </div>
            </div>
          </div>
      </div>
      <script type="text/javascript">
          $(function () {
              $('#guestgteqcreated_atPicker').datetimepicker({
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#guestlteqcreated_atPicker').datetimepicker({
                  useCurrent: false,
                  format: 'YYYY-MM-DD hh:mm:SS'
              });
              $('#guestgteqcreated_atPicker').on("dp.change", function (e) {
                  $('#guestlteqcreated_atPicker').data("DateTimePicker").minDate(e.date);
              });
              $('#guestlteqcreated_atPicker').on("dp.change", function (e) {
                  $('#guestgteqcreated_atPicker').data("DateTimePicker").maxDate(e.date);
              });
          });
      </script>

      <%= f.submit :class => "btn btn-primary btn-block mb-1" %>

      <a href="/guesthostprofiles" class="btn btn-default btn-block mb-1">Clear filters</a>
    <% end %>
    <a class="btn btn-default btn-block mb-1" data-toggle="collapse" href="#guesthostprofiles_filters">
      Show Filters
    </a>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Host</th>
        <th>User guest</th>
        <th>Actions</th>
      </tr>

      <% @guesthostprofiles.each do |guesthostprofile| %>
      <tr>
        <td>
          <% if guesthostprofile.host.present? %>
            <a href="/user_hosts/<%= guesthostprofile.host.id %>">
              <%= guesthostprofile.host.created_at %>
            </a>
          <% end %>
        </td>
        <td>
          <% if guesthostprofile.guest.present? %>
            <a href="/user_guests/<%= guesthostprofile.guest.id %>">
              <%= guesthostprofile.guest.created_at %>
            </a>
          <% end %>
        </td>
        <td>
          <a href="/guesthostprofiles/<%= guesthostprofile.id %>" class="btn btn-primary">Show</a>
          <a href="/guesthostprofiles/<%= guesthostprofile.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_guesthostprofile/<%= guesthostprofile.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
    <%= paginate @guesthostprofiles, theme: 'twitter-bootstrap-4' %>
  </div>
</div>
