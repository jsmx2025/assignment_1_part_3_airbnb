<div class="page-header">
  <h3>H Tag Rating #<%= @h_tag_rating.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Overall rating</dt>
      <dd><%= @h_tag_rating.overall_rating %></dd>

      <dt>Rating</dt>
      <dd>
        <% if @h_tag_rating.rating.present? %>
          <a href="/ratings/<%= @h_tag_rating.rating_id %>">
            <%= @h_tag_rating.rating.created_at %>
          </a>
        <% end %>
      </dd>

      <dt>Guest</dt>
      <dd>
        <% if @h_tag_rating.guest.present? %>
          <a href="/guestprofiles/<%= @h_tag_rating.guest_id %>">
            <%= @h_tag_rating.guest.guest_id %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/h_tag_ratings" class="btn btn-primary">
        Back
      </a>
      <a href="/h_tag_ratings/<%= @h_tag_rating.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_h_tag_rating/<%= @h_tag_rating.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
